<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MotorAsinBasketRobotProject.UI</title>
    <link rel="stylesheet" href="~/WebDesign/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/WebDesign/fonts/style.css">
    <link rel="stylesheet" href="~/WebDesign/css/main.css">
    <link rel="stylesheet" href="~/WebDesign/vendor/megamenu/css/megamenu.css">
    <link rel="stylesheet" href="~/WebDesign/vendor/search-filter/search-filter.css">
    <link rel="stylesheet" href="~/WebDesign/vendor/search-filter/custom-search-filter.css">
    <style>

        .content-wrapper::-webkit-scrollbar {
            width: .6em;
            border-radius: 50%;
            width: 8px;
        }

        .content-wrapper::-webkit-scrollbar-track {
            box-shadow: inset 0 0 6px rgba(0, 0, 0, 0.3);
        }

        .content-wrapper::-webkit-scrollbar-thumb {
            background: linear-gradient(45deg, #e02539, #e02539);
            border-radius: 0px 20px 0px 0px;
        }

        .content-wrapper {
            background: whitesmoke;
            overflow-y: scroll;
            padding: 3em;
            border-radius: 10px 10px 10px 10px;
            border-left: .3px solid rgba(224, 37, 57);
            border-top: .3px solid rgba(224, 37, 57);
            border-bottom: .3px solid rgba(224, 37, 57);
        }
    </style>
</head>
<body>

    <div id="loading-wrapper">
        <div class="spinner-border"></div>
        Loading...
    </div>
    <div class="page-wrapper">
        @await Component.InvokeAsync("ClientSideMenus")
        <div class="main-container">
            <div class="page-header">
                <div class="row gutters">
                    <div class="col-xl-8 col-lg-8 col-md-8 col-sm-6 col-9">
                        @await Component.InvokeAsync("ClientSearch")
                    </div>
                    <div class="col-xl-4 col-lg-4 col-md-4 col-sm-6 col-3">
                        <ul class="header-actions">
                            @await Component.InvokeAsync("ClientTasks")
                            <li class="dropdown">
                                <a href="#" id="notifications" data-toggle="dropdown" aria-haspopup="true">
                                    <i class="icon-alert-triangle"></i>
                                </a>
                                <div class="dropdown-menu dropdown-menu-end lrg" aria-labelledby="notifications">
                                    <div class="dropdown-menu-header">
                                        Notifications (7)
                                    </div>
                                    <div class="customScroll">
                                        <ul class="header-notifications">
                                            <li>
                                                <a href="#">
                                                    <div class="user-img online">
                                                        <img src="~/WebDesign/img/user6.png" alt="User">
                                                    </div>
                                                    <div class="details">
                                                        <div class="user-title">Larkyn</div>
                                                        <div class="noti-details">Check out every table in detail.</div>
                                                        <div class="noti-date">April 25, 04:00 pm</div>
                                                    </div>
                                                </a>
                                            </li>
                                            <li>
                                                <a href="#">
                                                    <div class="user-img busy">
                                                        <img src="~/WebDesign/img/user10.png" alt="User">
                                                    </div>
                                                    <div class="details">
                                                        <div class="user-title">Braxten</div>
                                                        <div class="noti-details">Approved new design.</div>
                                                        <div class="noti-date">April 10, 12:00 am</div>
                                                    </div>
                                                </a>
                                            </li>
                                            <li>
                                                <a href="#">
                                                    <div class="user-img away">
                                                        <img src="~/WebDesign/img/user21.png" alt="User">
                                                    </div>
                                                    <div class="details">
                                                        <div class="user-title">Maria</div>
                                                        <div class="noti-details">Membership has been ended.</div>
                                                        <div class="noti-date">March 20, 07:30 pm</div>
                                                    </div>
                                                </a>
                                            </li>
                                            <li>
                                                <a href="#">
                                                    <div class="user-img busy">
                                                        <img src="~/WebDesign/img/user15.png" alt="User">
                                                    </div>
                                                    <div class="details">
                                                        <div class="user-title">Alex</div>
                                                        <div class="noti-details">Design Review.</div>
                                                        <div class="noti-date">April 18, 09:30 am</div>
                                                    </div>
                                                </a>
                                            </li>
                                            <li>
                                                <a href="#">
                                                    <div class="user-img online">
                                                        <img src="~/WebDesign/img/user5.png" alt="User">
                                                    </div>
                                                    <div class="details">
                                                        <div class="user-title">Sunny</div>
                                                        <div class="noti-details">UI Discussion</div>
                                                        <div class="noti-date">April 21, 05:00 pm</div>
                                                    </div>
                                                </a>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </li>
                            @await Component.InvokeAsync("ClientUserSettings")
                        </ul>
                    </div>
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
    <script src="~/WebDesign/vendor/apex/apexcharts.min.js"></script>
        <script src="~/WebDesign/js/jquery.min.js"></script>
    <script src="~/WebDesign/js/bootstrap.bundle.min.js"></script>
    <script src="~/WebDesign/js/modernizr.js"></script>
    <script src="~/WebDesign/js/moment.js"></script>

    <!-- Megamenu JS -->
    <script src="~/WebDesign/vendor/megamenu/js/megamenu.js"></script>
    <script src="~/WebDesign/vendor/megamenu/js/custom.js"></script>

    <!-- Slimscroll JS -->
    <script src="~/WebDesign/vendor/slimscroll/slimscroll.min.js"></script>
    <script src="~/WebDesign/vendor/slimscroll/custom-scrollbar.js"></script>

    <!-- Search Filter JS -->
    <script src="~/WebDesign/vendor/search-filter/search-filter.js"></script>
    <script src="~/WebDesign/vendor/search-filter/custom-search-filter.js"></script>

    <!-- Apex Charts -->


    <!-- Area Graphs -->
    <script src="~/WebDesign/vendor/apex/examples/area/basic-area-graph.js"></script>
    <script src="~/WebDesign/vendor/apex/examples/area/basic-area-graph-spline.js"></script>
    <script src="~/WebDesign/vendor/apex/examples/area/basic-area-graph-stacked.js"></script>

    <!-- Bar Graphs -->
    <script src="~/WebDesign/vendor/apex/examples/bar/basic-bar-graph.js"></script>
    <script src="~/WebDesign/vendor/apex/examples/bar/basic-bar-graph-grouped.js"></script>
    <script src="~/WebDesign/vendor/apex/examples/bar/basic-bar-stack-graph.js"></script>
    <script src="~/WebDesign/vendor/apex/examples/bar/basic-bar-stack-graph-full-width.js"></script>
    <script src="~/WebDesign/vendor/apex/examples/bar/basic-bar-negative-values.js"></script>

    <!-- Candlestick Graphs -->
    <script src="~/WebDesign/vendor/apex/examples/candlestick/basic-candlestick-graph.js"></script>

    <!-- Column Graphs -->
    <script src="~/WebDesign/vendor/apex/examples/column/basic-column-graph.js"></script>
    <script src="~/WebDesign/vendor/apex/examples/column/basic-column-graph-datalables.js"></script>
    <script src="~/WebDesign/vendor/apex/examples/column/basic-column-stack-graph.js"></script>
    <script src="~/WebDesign/vendor/apex/examples/column/basic-column-stack-graph-fullheight.js"></script>

    <!-- Line Graphs -->
    <script src="~/WebDesign/vendor/apex/examples/line/basic-line-graph.js"></script>
    <script src="~/WebDesign/vendor/apex/examples/line/line-with-data-labels.js"></script>
    <script src="~/WebDesign/vendor/apex/examples/line/stepline.js"></script>

    <!-- Donut Graphs -->
    <script src="~/WebDesign/vendor/apex/examples/pie/basic-donut-graph.js"></script>
    <script src="~/WebDesign/vendor/apex/examples/pie/basic-donut-graph-gradient.js"></script>
    <script src="~/WebDesign/vendor/apex/examples/pie/basic-donut-graph-monochrome-gradient.js"></script>
    <script src="~/WebDesign/vendor/apex/examples/pie/basic-donut-graph-monochrome.js"></script>

    <!-- Pie Graphs -->
    <script src="~/WebDesign/vendor/apex/examples/pie/basic-pie-graph.js"></script>
    <script src="~/WebDesign/vendor/apex/examples/pie/basic-pie-graph-gradient.js"></script>
    <script src="~/WebDesign/vendor/apex/examples/pie/basic-pie-graph-monochrome.js"></script>
    <script src="~/WebDesign/vendor/apex/examples/pie/basic-pie-graph-monochrome-gradient.js"></script>

    <script src="~/WebDesign/js/main.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
